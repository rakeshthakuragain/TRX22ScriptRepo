def gv
pipeline {
    agent none

    stages {
		stage ('init'){
			steps {
				script{
					gv = load "script.groovy"
				}
			}
		}
        stage('Build') {
            agent { label 'di2inpun0050lv0' }
            steps {
				script{
					gv.buildUnit() 
				}					
            }
        }
    }

    post {
        success {
            echo 'This will run only if successful'
        }
    }
}
